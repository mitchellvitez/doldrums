three = if (2 == 3 || 3 < 4) 3 7;
four = ((1 + 6 - 2) * 4) / 5;

pair x y f = f x y;
fst p = p K;
snd p = p K1;

f x y =
  let a = pair x b
    , b = pair y a
  in
    fst $ snd $ snd $ snd a;

main = f three four;

fib n =
  if (n == 0) 1 $
  if (n == 1) 1 $
  fib (n - 1) + fib (n - 2);

